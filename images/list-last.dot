digraph G {
    node [ shape=box, fontname="PT Mono", fontsize=32 ]

    list [
        shape=none
        label=<<table border="0" cellspacing="0" cellpadding="5">
            <tr>
                <td border="1">1</td>
                <td border="1">2</td>
                <td border="1">3</td>
                <td border="1">5</td>
                <td border="1">7</td>
                <td port="last" border="1" bgcolor="yellow">11</td>
            </tr>
        </table>>
    ]

    last -> list:last

    last  [ label="-1st\nlast", style=filled, fillcolor=yellow ]
}
